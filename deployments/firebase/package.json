{
    "name": "@feuertiger/firebase-functions",
    "scripts": {
        "build": "npm i firebase-functions --no-save --no-package-lock",
        "serve": "npm run build && firebase serve --only functions",
        "shell": "npm run build && firebase functions:shell",
        "start": "npm run shell",
        "deploy": "firebase deploy --only functions",
        "logs": "npm login --registry=https://npm.pkg.github.com:_authToken=TOKEN firebase functions:log"
    },
    "main": "./dist/index.js",
    "dependencies": {
        "@feuertiger/proxy-graphql": "1.0.9-dev.08e9cba.0",
        "@feuertiger/web-client": "1.0.9-dev.08e9cba.0",
        "firebase-admin": "8.8.0",
        "firebase-functions": "3.3.0"
    },
    "devDependencies": {
        "typescript": "3.7.3"
    },
    "private": true
}
