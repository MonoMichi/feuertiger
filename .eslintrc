{
    "extends": [
        "airbnb-typescript",
        "prettier",
        "prettier/react",
        "prettier/@typescript-eslint"
    ],
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "rules": {
        "comma-dangle": 0,
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto"
            }
        ],
        "import/no-cycle": 0,
        "react/jsx-props-no-spreading": 0,
        "import/prefer-default-export": 0
    },
    "plugins": [
        "prettier",
        "eslint-plugin-import",
        "eslint-plugin-jsx-a11y",
        "eslint-plugin-react",
        "eslint-plugin-react-hooks"
    ],
    "globals": {
        "cy": true,
        "Cypress": true
    }
}
