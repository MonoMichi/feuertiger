{
    "window.zoomLevel": 0,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "2077",
    "workbench.preferredDarkColorTheme": "Material Theme",
    "files.autoSave": "off",
    "files.associations": {
        "*.md": "mdx"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.renderWhitespace": "all",
    "editor.renderControlCharacters": true,
    "editor.formatOnSave": false,
    "editor.formatOnPaste": true,
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
    "editor.fontFamily": "Fira Code",
    "editor.codeActionsOnSave": {
        "source.fixAll": false,
        "source.fixAll.eslint": false,
        "source.organizeImports": false,
        "planbcoding.vscode-react-refactor": false
    },
    "typescript.tsdk": "./node_modules/typescript/lib",
    "vscode-postgres.defaultDatabase": "feuertiger",
    "vscode-postgres.defaultConnection": "feuertiger",
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[yaml]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "git.enableSmartCommit": true,
    "terminal.integrated.enableBell": true,
    "terminal.integrated.inheritEnv": true,
    "terminal.integrated.shell.linux": "/bin/zsh",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "files.eol": "\n",
    "diffEditor.renderSideBySide": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "vs-kubernetes": {
        "vs-kubernetes.minikube-path.linux": "/root/.vs-kubernetes/tools/minikube/linux-amd64/minikube",
        "vs-kubernetes.helm-path.linux": "/root/.vs-kubernetes/tools/helm/linux-amd64/helm",
        "vs-kubernetes.draft-path.linux": "/root/.vs-kubernetes/tools/draft/linux-amd64/draft"
    },
    "eslint.format.enable": false,
    "eslint.options": {
        "config": "/workspaces/feuertiger/.eslintrc.js",
        "ResolvePluginsRelativeTo": "/workspaces/feuertiger",
        "noErrorOnUnmatchedPattern": true
    },
    "eslint.alwaysShowStatus": true,
    "vsintellicode.typescript.completionsEnabled": true,
    "typescript.tsserver.useSeparateSyntaxServer": true,
    "typescript.implementationsCodeLens.enabled": true,
    "typescript.tsserver.experimental.enableProjectDiagnostics": true,
    "typescript.tsserver.maxTsServerMemory": 4096,
    "typescript.workspaceSymbols.scope": "currentProject",
    "sqltools.connections": [
        {
            "previewLimit": 50,
            "server": "localhost",
            "port": 5432,
            "driver": "PostgreSQL",
            "name": "feuertiger-db",
            "database": "feuertiger",
            "username": "feuertiger",
            "password": "feuertiger"
        }
    ],
    "jestTestExplorer.pathToJest": "/workspaces/feuertiger/node_modules/.bin/jest",
    "jest.runAllTestsFirst": false,
    "jest.autoEnable": false,
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/.hg/store/**": true
    },
    "cSpell.words": [
        "ACMEDNS",
        "Autoscaler",
        "Brasten",
        "CRDs",
        "Callsite",
        "Categorys",
        "Chainable",
        "DIGITALOCEAN",
        "DMMF",
        "Datasource",
        "Dropzone",
        "Execa",
        "Feuertiger",
        "Fira",
        "Formik",
        "GITLAB",
        "Kabaddi",
        "PULUMI",
        "Penin",
        "Seon",
        "Serializers",
        "Talmor",
        "Thanei",
        "Unmount",
        "VCPU",
        "Whatshot",
        "Wook",
        "anseki",
        "apollographql",
        "automock",
        "azuretools",
        "backoff",
        "camelcase",
        "certmanager",
        "cloudvision",
        "clsx",
        "codeowners",
        "datasources",
        "dbaeumer",
        "devcontainer",
        "dind",
        "dockerconfigjson",
        "doctl",
        "doks",
        "drawio",
        "eamodio",
        "entrypoint",
        "equinusocio",
        "esbenp",
        "espree",
        "gitlens",
        "gsub",
        "hbenl",
        "hediet",
        "inplace",
        "jasonnutter",
        "jpoissonnier",
        "kavod",
        "kube",
        "kubeconfig",
        "kubernetes",
        "kubernetesx",
        "lazylog",
        "lcov",
        "letsencrypt",
        "loadbalancer",
        "michi",
        "mina",
        "minikube",
        "msjsdiag",
        "mtxr",
        "musl",
        "nextjs",
        "nixos",
        "nohoist",
        "notistack",
        "orta",
        "planbcoding",
        "postgres",
        "postgresql",
        "preid",
        "prettierignore",
        "prettierrc",
        "ptrace",
        "randomcolor",
        "rbac",
        "rhel",
        "rvest",
        "seccomp",
        "silvenon",
        "tesseract",
        "tomoki",
        "unfetch",
        "unmocked",
        "upsert",
        "upserts",
        "uuidv",
        "visualstudioexptteam",
        "vscodeintellicode",
        "vsintellicode",
        "vsliveshare"
    ],
    "cSpell.language": "en,de,de-DE,en-GB",
    "cSpell.ignorePaths": [
        "**/package-lock.json",
        "**/node_modules/**",
        "**/vscode-extension/**",
        "**/.vscode-server/**",
        "**/.git/objects/**",
        "**/dist/**",
        ".vscode",
        ".vscode-insiders",
        "yarn.lock"
    ]
}
