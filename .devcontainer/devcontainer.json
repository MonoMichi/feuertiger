{
    "name": "feuertiger",
    "dockerComposeFile": "../.dev-env.docker-compose.yml",
    "workspaceFolder": "/workspace",
    "service": "dev-env",
    "postCreateCommand": "yarn init",
    "forwardPorts": [3000, 4000, 5000, 5432],
    "settings": {
        "window.zoomLevel": 1,
        "workbench.iconTheme": "material-icon-theme",
        "workbench.colorTheme": "Material Theme",
        "workbench.preferredDarkColorTheme": "Material Theme",
        "files.autoSave": "off",
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.renderWhitespace": "all",
        "editor.renderControlCharacters": true,
        "editor.formatOnSave": false,
        "editor.formatOnPaste": true,
        "editor.fontLigatures": true,
        "editor.fontSize": 14,
        "editor.fontFamily": "Fira Code",
        "editor.codeActionsOnSave": {
            "source.fixAll": false,
            "source.fixAll.eslint": false,
            "source.organizeImports": false,
            "planbcoding.vscode-react-refactor": false
        },
        "typescript.tsdk": "./node_modules/typescript/lib",
        "vscode-postgres.defaultDatabase": "feuertiger",
        "vscode-postgres.defaultConnection": "feuertiger",
        "[typescript]": {
            "editor.defaultFormatter": "esbenp.prettier-vscode"
        },
        "[json]": {
            "editor.defaultFormatter": "esbenp.prettier-vscode"
        },
        "[javascript]": {
            "editor.defaultFormatter": "esbenp.prettier-vscode"
        },
        "[jsonc]": {
            "editor.defaultFormatter": "esbenp.prettier-vscode"
        },
        "[yaml]": {
            "editor.defaultFormatter": "esbenp.prettier-vscode"
        },
        "[dockerfile]": {
            "editor.defaultFormatter": "ms-azuretools.vscode-docker"
        },
        "git.enableSmartCommit": true,
        "terminal.integrated.enableBell": true,
        "terminal.integrated.inheritEnv": true,
        "terminal.integrated.shell.linux": "/bin/zsh",
        "typescript.updateImportsOnFileMove.enabled": "always",
        "javascript.updateImportsOnFileMove.enabled": "always",
        "files.eol": "\n",
        "diffEditor.renderSideBySide": false,
        "diffEditor.ignoreTrimWhitespace": false,
        "vs-kubernetes": {
            "vs-kubernetes.minikube-path.linux": "/root/.vs-kubernetes/tools/minikube/linux-amd64/minikube",
            "vs-kubernetes.helm-path.linux": "/root/.vs-kubernetes/tools/helm/linux-amd64/helm",
            "vs-kubernetes.draft-path.linux": "/root/.vs-kubernetes/tools/draft/linux-amd64/draft"
        },
        "eslint.format.enable": false,
        "eslint.options": {
            "config": "/workspaces/feuertiger/.eslintrc.js",
            "ResolvePluginsRelativeTo": "/workspaces/feuertiger",
            "noErrorOnUnmatchedPattern": true
        },
        "eslint.alwaysShowStatus": true,
        "vsintellicode.typescript.completionsEnabled": true,
        "typescript.tsserver.useSeparateSyntaxServer": true,
        "typescript.implementationsCodeLens.enabled": true,
        "typescript.tsserver.experimental.enableProjectDiagnostics": true,
        "typescript.tsserver.maxTsServerMemory": 4096,
        "typescript.workspaceSymbols.scope": "currentProject",
        "sqltools.connections": [
            {
                "previewLimit": 50,
                "server": "localhost",
                "port": 5432,
                "driver": "PostgreSQL",
                "name": "feuertiger-db",
                "database": "feuertiger",
                "username": "feuertiger",
                "password": "feuertiger"
            }
        ],
        "jestTestExplorer.pathToJest": "/workspaces/feuertiger/node_modules/.bin/jest",
        "jest.runAllTestsFirst": false,
        "files.watcherExclude": {
            "**/.git/objects/**": true,
            "**/.git/subtree-cache/**": true,
            "**/.hg/store/**": true
        }
    },
    "extensions": [
        "ms-vsliveshare.vsliveshare",
        "ms-vsliveshare.vsliveshare-audio",
        "hediet.vscode-drawio",
        "apollographql.vscode-apollo",
        "ms-vscode.vscode-typescript-next",
        "gitlab.gitlab-workflow",
        "Prisma.prisma",
        "Prisma.vscode-graphql",
        "anseki.vscode-color",
        "dbaeumer.vscode-eslint",
        "hbenl.vscode-test-explorer",
        "eamodio.gitlens",
        "Orta.vscode-jest",
        "mtxr.sqltools",
        "mtxr.sqltools-driver-pg",
        "kavod-io.vscode-jest-test-adapter",
        "eg2.vscode-npm-script",
        "esbenp.prettier-vscode",
        "jpoissonnier.vscode-styled-components",
        "Equinusocio.vsc-material-theme",
        "ms-azuretools.vscode-docker",
        "rvest.vs-code-prettier-eslint",
        "ms-kubernetes-tools.vscode-kubernetes-tools",
        "msjsdiag.debugger-for-chrome",
        "tomoki1207.pdf",
        "visualstudioexptteam.vscodeintellicode",
        "PKief.material-icon-theme",
        "planbcoding.vscode-react-refactor",
        "jasonnutter.vscode-codeowners"
    ]
}
